<?xml version="1.0"?>
<ruleset name="Biplane">
 <description>Biplane's coding standard</description>

 <exclude-pattern>Tests/*</exclude-pattern>

 <!-- This standard inherits of Symfony2 coding standard (https://github.com/opensky/Symfony2-coding-standard) -->
 <rule ref="Symfony2">
  <exclude name="Generic.Formatting.SpaceAfterCast" />
  <rule ref="Generic.Formatting.NoSpaceAfterCast" />
 </rule>
</ruleset>
