{
  "name": "biplane/yandex-direct",
  "type": "library",
  "require": {
    "php": ">=5.3.3",
    "symfony/options-resolver": "~2.3",
    "symfony/event-dispatcher": "~2.3",
    "kriswallsmith/buzz": "~0.12"
  },
  "require-dev": {
    "php": ">=5.4",
    "symfony/console": "~2.3",
    "symfony/framework-bundle": "~2.3",
    "lstrojny/phpunit-function-mocker": "^0.2.0",
    "wsdl2phpgenerator/wsdl2phpgenerator": "^3.2",
    "zendframework/zend-code": "^2.5",
    "doctrine/inflector": "^1.0"
  },
  "autoload": {
    "psr-4": { "Biplane\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Biplane\\Tests\\": "tests/" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  }
}
