{
  "name": "biplane/yandex-direct",
  "type": "library",
  "require": {
    "php": ">=5.3.3",
    "symfony/options-resolver": "~2.1",
    "kriswallsmith/buzz": "~0.12"
  },
  "require-dev": {
    "symfony/console": "~2.1",
    "symfony/framework-bundle": "~2.2"
  },
  "autoload": {
    "psr-4": { "Biplane\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Biplane\\Tests\\": "tests/" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  }
}
