{
  "name": "biplane/yandex-direct",
  "type": "library",
  "description": "PHP library for Yandex.Direct API",
  "keywords": ["yandex", "yandex.direct", "Яндекс.Директ", "sdk"],
  "authors": [
    {
      "name": "Denis",
      "email": "liquid.yethee@hotmail.com"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/biplane/yandex-direct/graphs/contributors"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^5.5|^7.0",
    "symfony/options-resolver": "~2.3|~3.0",
    "symfony/event-dispatcher": "~2.3|~3.0",
    "kriswallsmith/buzz": "~0.12"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8|~5.5",
    "symfony/console": "~2.3",
    "lstrojny/phpunit-function-mocker": "^0.3.0|^0.4.0",
    "wsdl2phpgenerator/wsdl2phpgenerator": "^3.2",
    "zendframework/zend-code": "^2.5",
    "doctrine/inflector": "^1.0"
